@media screen and (-webkit-min-device-pixel-ratio:1.0) {
  .rating-star {
    background-image: url(data:image/png;base64,);
  }
}

@media screen and (-webkit-min-device-pixel-ratio:2.0) {
  .rating-star {
    background-image: url(data:image/png;base64,);
    /* background-size: 55px 121px; */
    background-size: 110rpx 242rpx;
  }
}

.rating {
  display: flex;
  flex-direction: row;
  position: relative;
  /* margin: 10rpx 0; */
}

.rating-star {
  content: "";
  display: inline-block;
  /* width: 55px;
  height: 11px; */
  width: 110rpx;
  /* height: 22rpx; */
  height: 21rpx;
  position: absolute;
  background-repeat: no-repeat;
}

